{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "*.{html,js,css,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot,json,wasm,dat}",
      "use": "@vercel/static"
    },
    {
      "src": "{assets,canvaskit,icons}/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|wasm|dat))",
      "dest": "/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/canvaskit/(.*)",
      "dest": "/canvaskit/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
